<!--Container Fluid, con autoResize - Resto página (sin header) -->
<div class="container-fluid">
  <!--Título y alerts  -->
  <div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8"><span class="text-title-blue"><%= t(:editing_profile) %></span></div>
    <div class="col-md-2"></div>
  </div>
  <!--/.Título y alerts  -->

  <!-- ListGroup Accessos, filtros y tabla derecha-->
  <div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-2">
      <!-- ListGroup Accesos Directos y filtros-->
      <div class="list-group">
        <div class="list-group-item active text-normal"><b><%= t(:what_info) %></b></div>
        <div class="list-group-item disabled text-normal"><%= t(:profile_form_aclaration) %></div>
        <div class="list-group-item disabled text-normal"><%= t(:blank_form_aclaration) %></div>
        <div class="list-group-item disabled text-normal"><%= t(:feedback) %></div>
</div>
</div>
    <!-- /.ListGroup Accesos Directos y filtros-->
    <!-- Tabla detalle -->
    <div class="col-md-6">
      <%= render "form" %>
    </div>
    <!-- ./Tabla detalle -->
    <div class="col-md-2"></div>
  </div>
  <!-- /.ListGroup Accessos, filtros y tabla derecha-->

</div>
<!-- /.Container Fluid, con autoResize - Resto página (sin header) -->
