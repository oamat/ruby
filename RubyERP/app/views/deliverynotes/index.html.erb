<h1>Listing deliverynotes</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th>Client</th>
      <th>Address</th>
      <th>Address</th>
      <th>Tax</th>
      <th>Tax</th>
      <th>Discount</th>
      <th>Discount</th>
      <th>Payment</th>
      <th>Date</th>
      <th>Observations</th>
      <th>Expiration</th>
      <th>Total</th>
      <th>Tariff code</th>
      <th>Gross load</th>
      <th>Net load</th>
      <th>Receiver</th>
      <th>Sender</th>
      <th>Has invoice</th>
      <th>Has forecast</th>
      <th>Has order</th>
      <th>Has delivery</th>
      <th>Invoice</th>
      <th>Forecast</th>
      <th>Order</th>
      <th>Deliverynote</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deliverynotes.each do |deliverynote| %>
      <tr>
        <td><%= deliverynote.company_id %></td>
        <td><%= deliverynote.client_id %></td>
        <td><%= deliverynote.address_id %></td>
        <td><%= deliverynote.address %></td>
        <td><%= deliverynote.tax_id %></td>
        <td><%= deliverynote.tax %></td>
        <td><%= deliverynote.discount_id %></td>
        <td><%= deliverynote.discount %></td>
        <td><%= deliverynote.payment %></td>
        <td><%= deliverynote.date %></td>
        <td><%= deliverynote.observations %></td>
        <td><%= deliverynote.expiration %></td>
        <td><%= deliverynote.total %></td>
        <td><%= deliverynote.tariff_code %></td>
        <td><%= deliverynote.gross_load %></td>
        <td><%= deliverynote.net_load %></td>
        <td><%= deliverynote.receiver %></td>
        <td><%= deliverynote.sender %></td>
        <td><%= deliverynote.has_invoice %></td>
        <td><%= deliverynote.has_forecast %></td>
        <td><%= deliverynote.has_order %></td>
        <td><%= deliverynote.has_delivery %></td>
        <td><%= deliverynote.invoice_id %></td>
        <td><%= deliverynote.forecast_id %></td>
        <td><%= deliverynote.order_id %></td>
        <td><%= deliverynote.deliverynote_id %></td>
        <td><%= link_to t(:show), deliverynote %></td>
        <td><%= link_to t(:edit), edit_deliverynote_path(deliverynote) %></td>
        <td><%= link_to t(:destroy), deliverynote, method: :delete, data: { confirm: t(:confirm) } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Deliverynote', new_deliverynote_path %>
